<h1>Все автомобили</h1>
<h3>@Model.carCategory</h3>
<div class="row mt-5 mb-2">
    @{
        if (Model.allCars != null)
        {
            foreach(Car car in Model.allCars){ 
                @Html.Partial("AllCars",car)
            }
        }
        else
        {
            <p>Нет доступных автомобилей в этой категории</p>
        }
    }
</div>